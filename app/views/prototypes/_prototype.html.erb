<div class="card">
  <%= link_to prototype_path(prototype) do %>
    <%= image_tag prototype.image, class: 'prototype-image' if prototype.image.attached? %>
  <% end %>

  <div class="card__body">
    <%#= link_to prototype.title, root_path, class: :card__title%>
    <%= link_to prototype.title, user_path(prototype.user), class: :card__title %>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%#= link_to "by#{prototype.user.name} ", root_path, class: :card__user %>
    <%= link_to "by#{prototype.user.name} ", user_path(prototype.user), class: :card__user %>
  </div>
</div>